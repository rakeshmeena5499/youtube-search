<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Video Dashboard</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
        }

        th, td {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }
    </style>
</head>
<body>
    <h1>Video Dashboard</h1>
    <table>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Channel Title</th>
            <th>Published At</th>
            <th>Thumbnail</th>
        </tr>
        <% videos.forEach(video => { %>
            <tr>
                <td><%= video.title %></td>
                <td><%= video.description %></td>
                <td><%= video.channelTitle %></td>
                <td><%= video.publishedAt %></td>
                <td><img src="<%= video.thumbnails.default.url %>" alt="Thumbnail"></td>
            </tr>
        <% }); %>
    </table>
</body>
</html>
